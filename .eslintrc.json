{
    "env": {
        "browser": true,
        "node": true,
        "jasmine": true
    },
    "parser": "babel-eslint",
    "plugins": ["prettier"],
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "rules": {
        "react/prop-types": 0,
        "no-endef": "off",
        "no-unused-vars": 1,
        "no-console": 0,
        "react/jsx-filename-extension": 0,
        "prettier/prettier": ["error",
            { 
                "endOfLine": "auto"
            }
        ]
    }
}
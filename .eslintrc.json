{
    "env": {
        "browser": true,
        "node": true,
        "jasmine": true
    },
    "parser": "babel-eslint",
    "plugins": [
        "prettier"
    ],
    "extends": ["airbnb", "prettier"],
    "rules": {
        "react/prop-types": 0,
        "no-endef": "off",
        "no-unused-vars": 1,
        "no-console": 0,
        "react/jsx-filename-extension": 0,
        "prettier/prettier": [
            "error",
            {
                "endOfLine": "auto"
            }
        ],
        "react/button-has-type":0,
        "react/prefer-stateless-function": 0,
        "import/no-unresolved": 0,
        "no-alert": 0,
        "no-plusplus": ["error", { "allowForLoopAfterthoughts": true }],
        "camelcase": 0
    }
}